              "uiDisplayName": "Market Rate per Unit",
              "fieldType": "currency",
              "isRequired": true,
              "sortOrder": 1
            },
            {
              "fieldId": "government_rate",
              "uiDisplayName": "Government Circle/Guidance Rate",
              "fieldType": "currency",
              "isRequired": true,
              "sortOrder": 2
            },
            {
              "fieldId": "valuation_method",
              "uiDisplayName": "Valuation Method Used",
              "fieldType": "select",
              "options": [
                {
                  "value": "comparative",
                  "label": "Comparative Market Analysis"
                },
                {
                  "value": "income",
                  "label": "Income Approach"
                },
                {
                  "value": "cost",
                  "label": "Cost Approach"
                },
                {
                  "value": "hybrid",
                  "label": "Hybrid Method"
                }
              ],
              "isRequired": true,
              "sortOrder": 3
            },
            {
              "fieldId": "estimated_market_value",
              "uiDisplayName": "Estimated Market Value",
              "fieldType": "calculated",
              "formula": "total_area * market_rate_per_unit",
              "displayFormat": "currency",
              "isReadonly": true,
              "sortOrder": 4
            }
          ]
        },
        {
          "sectionId": "adjustments",
          "sectionName": "Valuation Adjustments",
          "sortOrder": 2,
          "fields": [
            {
              "fieldId": "location_factor",
              "uiDisplayName": "Location Factor (%)",
              "fieldType": "decimal",
              "helpText": "Positive for premium locations, negative for disadvantaged",
              "sortOrder": 1
            },
            {
              "fieldId": "condition_factor",
              "uiDisplayName": "Property Condition Factor (%)",
              "fieldType": "decimal",
              "helpText": "Adjustment based on property condition",
              "sortOrder": 2
            },
            {
              "fieldId": "age_factor",
              "uiDisplayName": "Age Factor (%)",
              "fieldType": "decimal",
              "helpText": "Depreciation based on property age",
              "sortOrder": 3
            },
            {
              "fieldId": "final_adjusted_value",
              "uiDisplayName": "Final Adjusted Value",
              "fieldType": "calculated",
              "formula": "estimated_market_value + (estimated_market_value * (location_factor + condition_factor + age_factor) / 100)",
              "displayFormat": "currency",
              "isReadonly": true,
              "sortOrder": 4
            }
          ]
        }
      ],
      "uiName": "Property Valuation"
    },
